<?xml version="1.0"?>
<sdf version="1.7">
  <model name="limo_diff">
    <static>false</static>

    <link name="base_link">
      <inertial>
        <mass>2.2</mass>
        <inertia>
          <ixx>0.24</ixx>
          <iyy>0.96</iyy>
          <izz>0.96</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="base_visual">
        <pose>0 0 -0.15 0 0 1.5708</pose>
        <geometry>
          <mesh>
            <uri>model://limo_diff/meshes/limo_base.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="base_collision">
        <geometry>
          <box>
            <size>0.13 0.12 0.1</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name="front_left_wheel_link">
      <pose>0.1 0.065 -0.10 0 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01055</ixx>
          <iyy>0.00075</iyy>
          <izz>0.01055</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="wheel_visual">
        <geometry>
          <mesh>
            <uri>model://limo_diff/meshes/limo_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="wheel_collision">
        <pose>0 0.0225 0 1.5708 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.045</radius>
            <length>0.045</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.2</mu>
              <mu2>1.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name="front_right_wheel_link">
      <pose>0.1 -0.065 -0.10 3.14159 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01055</ixx>
          <iyy>0.00075</iyy>
          <izz>0.01055</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="wheel_visual">
        <geometry>
          <mesh>
            <uri>model://limo_diff/meshes/limo_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="wheel_collision">
        <pose>0 0.0225 0 1.5708 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.045</radius>
            <length>0.045</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.2</mu>
              <mu2>1.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name="rear_left_wheel_link">
      <pose>-0.1 0.065 -0.10 0 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01055</ixx>
          <iyy>0.00075</iyy>
          <izz>0.01055</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="wheel_visual">
        <geometry>
          <mesh>
            <uri>model://limo_diff/meshes/limo_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="wheel_collision">
        <pose>0 0.0225 0 1.5708 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.045</radius>
            <length>0.045</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.2</mu>
              <mu2>1.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name="rear_right_wheel_link">
      <pose>-0.1 -0.065 -0.10 3.14159 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01055</ixx>
          <iyy>0.00075</iyy>
          <izz>0.01055</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="wheel_visual">
        <geometry>
          <mesh>
            <uri>model://limo_diff/meshes/limo_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="wheel_collision">
        <pose>0 0.0225 0 1.5708 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.045</radius>
            <length>0.045</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.2</mu>
              <mu2>1.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <link name="depth_camera_link">
      <pose>0.12 0 0.12 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>1e-6</ixx>
          <iyy>1e-6</iyy>
          <izz>1e-6</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="camera_visual">
        <geometry>
          <box>
            <size>0.03 0.02 0.02</size>
          </box>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.1 0.1 0.1 1</diffuse>
        </material>
      </visual>
      <collision name="camera_collision">
        <geometry>
          <box>
            <size>0.03 0.02 0.02</size>
          </box>
        </geometry>
      </collision>
      <sensor name="rgb_camera" type="camera">
        <topic>rgb_camera</topic>
        <update_rate>30</update_rate>
        <frame_id>depth_camera_link</frame_id>
        <camera>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>1280</width>
            <height>720</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.1</near>
            <far>5.0</far>
          </clip>
        </camera>
        <always_on>true</always_on>
        <visualize>true</visualize>
      </sensor>
    </link>

    <joint name="front_left_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>front_left_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
          <effort>50</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.0</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="front_right_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>front_right_wheel_link</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
          <effort>50</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.0</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="rear_left_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>rear_left_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
          <effort>50</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.0</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="rear_right_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>rear_right_wheel_link</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
          <effort>50</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.0</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="depth_camera_joint" type="fixed">
      <parent>base_link</parent>
      <child>depth_camera_link</child>
    </joint>

    <plugin
      filename="ignition-gazebo-pose-publisher-system"
      name="gz::sim::systems::PosePublisher">
      <publish_link_pose>false</publish_link_pose>
      <use_pose_vector_msg>false</use_pose_vector_msg>
      <publish_nested_model_pose>true</publish_nested_model_pose>
      <update_frequency>50</update_frequency>
    </plugin>

    <plugin
      filename="ignition-gazebo-diff-drive-system"
      name="gz::sim::systems::DiffDrive">
      <topic>/model/limo_01/cmd_vel</topic>
      <odom_topic>/model/limo_01/odometry</odom_topic>
      <odom_publish_frequency>50</odom_publish_frequency>
      <frame_id>odom</frame_id>
      <child_frame_id>base_link</child_frame_id>
      <publish_tf>true</publish_tf>
      <left_joint>front_left_wheel_joint</left_joint>
      <left_joint>rear_left_wheel_joint</left_joint>
      <right_joint>front_right_wheel_joint</right_joint>
      <right_joint>rear_right_wheel_joint</right_joint>
      <wheel_separation>0.13</wheel_separation>
      <wheel_radius>0.045</wheel_radius>
      <min_velocity>-1</min_velocity>
      <max_velocity>1</max_velocity>
      <min_acceleration>-1</min_acceleration>
      <max_acceleration>1</max_acceleration>
      <max_torque>30</max_torque>
    </plugin>

    <plugin
      filename="ignition-gazebo-joint-state-publisher-system"
      name="gz::sim::systems::JointStatePublisher" />
  </model>
</sdf>
