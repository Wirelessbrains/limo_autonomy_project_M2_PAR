Panels:
  - Class: rviz_common/Displays
    Name: Displays
  - Class: rviz_common/Selection
    Name: Selection
  - Class: rviz_common/Tool Properties
    Name: Tool Properties
  - Class: rviz_common/Views
    Name: Views
Visualization Manager:
  Class: ""
  Displays:
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz_default_plugins/Grid
      Color: 160; 160; 164
      Enabled: true
      Name: Grid
      Plane: XZ
      Plane Cell Count: 40
      Reference Frame: map
      Value: true
    - Class: rviz_default_plugins/Path
      Enabled: true
      Name: Reference Path
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /relocalization/reference_path
      Color: 0; 255; 255
      Line Style: Lines
      Line Width: 0.03
      Value: true
    - Class: rviz_default_plugins/Pose
      Enabled: true
      Name: Current Pose
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /relocalization/current_pose
      Arrow Length: 0.5
      Axes Length: 0.25
      Axes Radius: 0.02
      Shape: Arrow
      Color: 255; 255; 0
      Value: true
    - Class: rviz_default_plugins/Pose
      Enabled: true
      Name: Nearest Pose
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /relocalization/nearest_pose
      Arrow Length: 0.45
      Axes Length: 0.2
      Axes Radius: 0.02
      Shape: Axes
      Color: 255; 64; 64
      Value: true
    - Class: rviz_default_plugins/MarkerArray
      Enabled: true
      Name: Relocalization Markers
      Namespaces:
        relocalization: true
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /relocalization/markers
      Value: true
  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: map
  Name: root
  Tools:
    - Class: rviz_default_plugins/Interact
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select
    - Class: rviz_default_plugins/FocusCamera
    - Class: rviz_default_plugins/Measure
    - Class: rviz_default_plugins/SetInitialPose
      Topic:
        Value: /initialpose
    - Class: rviz_default_plugins/SetGoal
      Topic:
        Value: /goal_pose
  Value: true
  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Distance: 12
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.75
      Target Frame: map
      Yaw: 0.75
Window Geometry:
  Displays:
    collapsed: false
  Height: 900
  Width: 1500
